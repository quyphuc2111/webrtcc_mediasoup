{
  "name": "screensharing-webrtc-mediasoup",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:student": "vite --config vite.config.student.ts",
    "build": "tsc && vite build",
    "build:student": "tsc && vite build --config vite.config.student.ts",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev:student": "tauri dev --config src-tauri/tauri.student.conf.json",
    "tauri:build": "npm run build && tauri build",
    "tauri:build:student": "npm run build:student && tauri build --config src-tauri/tauri.student.conf.json",
    "server:install": "cd mediasoup-server && npm install",
    "server:dev": "cd mediasoup-server && npm run dev",
    "server:build": "cd mediasoup-server && npm run build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "mediasoup-client": "^3.7.17"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "@tauri-apps/cli": "^2"
  }
}
